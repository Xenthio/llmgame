					m_sName = "Parking the Feet"
					m_vecPosition = [ -977.0, 589.0 ]
					m_nNodeID = 
					{
						m_id = 772624619
					}
					m_sNote = ""
					m_commentText = "Sometimes when transitioning between animations with very different movements, we want to disable any locks on the feet and just let them slide into the right position.  This is the case here when transitioning from Walking back to the Idle pose, as the feet can be in very different positions when the character reaches the end of the path, and the idle does not have any foot steps that would adjust the feet to the right place.  So here we enabled the \"Park Feet\" option on the transition from  Move to Idle to signal to the StrideR system  that should set both the previous and next foot positions to the position defined by the Idle animation, and blend the feet to that location"
					m_size = [ 375.0, 446.0 ]
					m_color = [ 30, 107, 135, 255 ]
